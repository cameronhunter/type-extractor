{
  "name": "@cameronhunter/type-extractor",
  "version": "0.0.0",
  "description": "Extract types that are embedded in a package to a separate @types package",
  "bin": {
    "type-extractor": "./build/bin/index.js"
  },
  "author": "Cameron Hunter <hello@cameronhunter.co.uk>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && chmod +x ./build/bin/index.js",
    "clean": "rm -rf build",
    "start": "yarn build && node ./build/bin/index.js",
    "test": "vitest"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "dedent": "^0.7.0",
    "minimatch": "^9.0.2",
    "tar": "^6.1.15",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.3",
    "@tsconfig/strictest": "^2.0.1",
    "@types/dedent": "^0.7.0",
    "@types/node": "~18",
    "@types/tar": "^6.1.5",
    "typescript": "^5.1.5",
    "vitest": "^0.32.2"
  },
  "packageManager": "yarn@3.6.0",
  "publishConfig": {
    "access": "public"
  }
}
